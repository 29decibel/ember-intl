{
  "name": "ember-intl",
  "description": "Ember toolbox for internationalization.",
  "version": "1.0.0-rc7",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test",
    "bower": "node_modules/bower/bin/bower install",
    "postinstall": "[ -f node_modules/bower/bin/bower ] && (npm run-script bower) || echo 'Bower not installed, skipping'"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yahoo/ember-intl.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "jason.mitchell.w@gmail.com",
  "license": "BSD-3-Clause",
  "dependencies": {
    "broccoli-filter": "^0.1.11",
    "broccoli-funnel": "^0.2.0",
    "cldr": "2.3.2",
    "intl-messageformat": "1.0.4",
    "intl-relativeformat": "1.0.3",
    "intl": "0.1.4",
    "serialize-javascript": "^1.0.0"
  },
  "devDependencies": {
    "bower": "1.3.12",
    "ember-cli": "0.1.12",
    "ember-cli-dependency-checker": "0.0.6",
    "ember-cli-htmlbars": "^0.6.0",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.1.2",
    "ember-code-snippet": "^0.1.3",
    "express": "^4.8.5",
    "path": "^0.11.14"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
